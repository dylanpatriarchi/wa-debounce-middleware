{
    "framework": "other",
    "buildCommand": "npm install",
    "installCommand": "npm install",
    "outputDirectory": "api",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/index.js"
        }
    ],
    "functions": {
        "api/**/*.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}